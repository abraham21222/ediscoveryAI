{
  "connectors": [
    {
      "type": "microsoft_graph",
      "name": "tenant_acme_m365",
      "enabled": false,
      "params": {
        "tenant_id": "00000000-0000-0000-0000-000000000000",
        "client_id": "11111111-1111-1111-1111-111111111111",
        "client_secret": "aws-secrets-manager://m365/acme/client_secret",
        "mailbox": "ediscovery@acmelegal.com",
        "batch_size": 500
      }
    },
    {
      "type": "google_workspace",
      "name": "tenant_orion_gsuite",
      "enabled": false,
      "params": {
        "service_account_file": "aws-secrets-manager://gsuite/orion/service_account.json",
        "subject_user": "ediscovery@orionlaw.com",
        "batch_size": 500
      }
    },
    {
      "type": "cloud_storage",
      "name": "box_enterprise_exports",
      "enabled": false,
      "params": {
        "provider": "box",
        "bucket": "s3://ediscovery-prod-box-staging",
        "prefix": "incoming/box/",
        "credential_secret": "aws-secrets-manager://box/enterprise/jwt"
      }
    },
    {
      "type": "mock_email",
      "name": "sample_seed_data",
      "enabled": true,
      "params": {
        "batch_size": 25
      }
    }
  ],
  "object_store": {
    "type": "local_fs",
    "params": {
      "base_path": "./_evidence"
    }
  },
  "metadata_store": {
    "type": "sqlite",
    "params": {
      "path": "./_metadata/metadata.db"
    }
  },
  "processing": {
    "enable_deduplication": true,
    "enable_ocr": true,
    "enable_entity_extraction": true,
    "enable_privilege_detection": false
  },
  "security": {
    "envelope_encryption": true,
    "key_management_service": "aws-kms://arn:aws:kms:us-east-1:123456789012:key/22222222-3333-4444-5555-666666666666",
    "rbac_policy": "iam-role://ingestion-pipeline",
    "audit_log_destination": "kinesis-firehose://ingestion-audit"
  }
}
